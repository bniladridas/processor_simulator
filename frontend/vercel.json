{
    "version": 2,
    "name": "processor-simulator",
    "builds": [
        { 
            "src": "package.json", 
            "use": "@vercel/static-build",
            "config": { 
                "distDir": "build",
                "buildCommand": "npm run build"
            }
        }
    ],
    "routes": [
        { "src": "/(.*)", "dest": "/index.html" }
    ]
}
